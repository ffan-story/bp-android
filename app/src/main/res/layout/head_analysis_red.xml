<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color_gray_f3"
    android:orientation="vertical">

    <TextView
        style="@style/txt_color33_size16"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/view_spacing_10"
        android:background="@color/white"
        android:padding="@dimen/view_spacing_10"
        android:text="@string/anal_red_charge_off_total" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dividerheight"
        android:background="@color/divider"
        android:clickable="false"
        android:focusable="false" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_size_75"
        android:background="@color/white"
        android:layout_gravity="center"
        android:gravity="center_vertical"
        >

        <TextView
            android:id="@+id/anal_tv_charge_off_total"
            style="@style/txt_color33_size18"
            android:layout_marginLeft="@dimen/view_spacing_10"
            android:lineSpacingExtra="@dimen/view_size_5"
            android:layout_centerVertical="true"/>

        <TextView
            android:id="@+id/line1"
            android:layout_width="@dimen/dividerheight"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:layout_margin="@dimen/view_size_5"
            android:background="@color/divider" />

        <TextView
            android:id="@+id/anal_tv_subsidy_money_ff"
            style="@style/txt_color33_size18"
            android:layout_marginLeft="@dimen/view_spacing_10"
            android:layout_toRightOf="@id/line1"
            android:lineSpacingExtra="@dimen/view_size_5"
            android:layout_centerVertical="true"/>

    </RelativeLayout>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dividerheight"
        android:background="@color/divider"
        android:clickable="false"
        android:focusable="false" />

    <RelativeLayout
        android:id="@+id/rel_2row"
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_size_75"
        android:background="@color/white"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/anal_tv_subsidy_money_third"
            style="@style/txt_color33_size18"
            android:layout_marginLeft="@dimen/view_spacing_10"
            android:lineSpacingExtra="@dimen/view_size_5"
            android:layout_centerVertical="true"/>

        <TextView
            android:id="@+id/line"
            android:layout_width="@dimen/dividerheight"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true"
            android:layout_margin="@dimen/view_size_5"
            android:background="@color/divider"
            android:clickable="false"
            android:focusable="false" />

        <TextView
            android:id="@+id/anal_tv_subsidy_money_vendor"
            style="@style/txt_color33_size18"
            android:layout_marginLeft="@dimen/view_spacing_10"
            android:layout_toRightOf="@id/line"
            android:lineSpacingExtra="@dimen/view_size_5"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="@dimen/dividerheight"
        android:background="@color/divider"
        android:clickable="false"
        android:focusable="false" />

    <TextView
        style="@style/txt_color33_size16"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/view_spacing_10"
        android:background="@color/white"
        android:padding="@dimen/view_spacing_10"
        android:text="@string/anal_red_sub_total" />


    <TextView
        android:id="@+id/no_data_view"
        style="@style/txt_color99_size16"
        android:paddingLeft="@dimen/view_spacing_8"
        android:layout_margin="@dimen/view_spacing_10"
        android:text="@string/anal_subsidy_no_data"/>
</LinearLayout>
